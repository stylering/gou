@import "mixins";
@import "util";

.hd{
	h1{margin-left:px(10);}
	a + h1{margin-left:0;}
	.voice{
		position:absolute; right:px(10); top:px(10); width:px(22); height:px(24);
		background:url("../img/amigo_ico_voice.jpg") no-repeat; background-size:contain;
	}
}

// 提交成功
.success-box{
	.tip{
		text-align:center; overflow:hidden;
		p:nth-of-type(1){color:#e05a3f; font-size:px(14); margin:px(10) 0 px(4);}
		.ico{
			margin-top:px(50);
			span{
				display:inline-block; border-radius:px(30); width:px(60); height:px(60); background:#f78069;
				&:before{
					content:""; display:block; width:px(24); height:px(12); border:solid #fff; border-width:0 0 4px 4px;
					transform:rotate(-45deg); margin:px(18) 0 0 px(16); 
				}
			}
			span.fail{
				&:before{display:none;}
				&:after{content:"×"; display:block; font-size:px(60); color:#fff;}
			}
		}
	}
	.web-btn{margin-top:px(72); a{width:px(272);}}
	.warning{width:px(272); margin:px(8) auto 0; line-height:1.5;}
}

// 创建订单
.goods-inf{
	width:px(320); margin:0 auto; border-bottom:solid 1px #cdcdcd; background:#fff;
	figure{display:flex;}
	.f-red{color:#b12828;}
	.pic{width:px(86); max-height:px(86); padding:px(8);}
	.desc{flex:1; padding-right:px(8); overflow:hidden;}
	dl{margin:px(8) 0; line-height:1.4;}
	dt{font-size:px(14);}
	.buy-price{
		font-size:px(14); text-align:right; border-top:solid 1px #f1f1f1; padding:px(11) 0 px(9); margin:0 px(8);
		em{font-size:px(18);}
	}
}

.input-inf{
	width:px(320); margin:0 auto;
	form{padding:0 px(8);}
	.item{
		h3{font-size:px(14); padding:px(8) 0 px(4);}
		fieldset{background:#fff; border-radius:3px; border:solid 1px #cdcdcd;}
		li{margin:px(8) 0; padding:0 px(4) 0 px(8);}
		label{font-size:px(14); line-height:px(36);}
		select{width:px(68); overflow:hidden;}
		input[type=text]{border:solid 1px #cbcbcb; border-radius:2px; font-size:px(14); padding:px(8) px(6); width:px(200);}
		i{display:inline-block; width:px(28);} b{display:inline-block; width:px(7);}
	}
	.pay-ways{
		li{
			display:flex; margin:0; padding:0;
			&:first-child{border-bottom:solid 1px #cdcdcd; label{background-image:url("../img/amigo_ico_onlinePay.jpg");}}
			&:last-child label{background-image:url("../img/amigo_ico_codPay.jpg");}
		}
		label{display:block; line-height:px(46); flex:1; padding-left:px(40); background:no-repeat px(12) px(14); background-size:px(18);}
		span{display:block; width:px(30); padding-top:px(14);}
	}
	.web-btn{margin-top:px(12); button{width:px(304);}}
}

// 商品详情
.detail-wrap{
	width:px(320); margin:0 auto; background:#fff;
	.f-red{color:#b12828;}
	.pic-text-price{
		p{
			font-size:px(14); line-height:1.6; margin:0 px(8);
			&:first-child{margin:0;}
			&:last-child{font-size:px(12); padding-bottom:px(10); border-bottom:solid 1px #f1f1f1; em{font-size:px(16);}}
			span{display:inline-block; color:#fff; font-size:px(10); padding:4px; line-height:1; margin:0 1px;}
		}
		.bg1{background:#db332b;}
		.bg2{background:#ef7f29;}
		.bg3{background:#1abc9c;}
	}
	.num{
		padding:px(8) 0; margin:0 px(8); border-bottom:solid 1px #f1f1f1;
		input{width:px(40); padding:px(6); border:solid 1px #e3e3e3; font-size:px(13); text-align:center; background-image:linear-gradient(top,#f2f2f2,#fff);}
		span{display:inline-block; vertical-align:middle; border:solid 1px #e3e3e3; width:px(26); height:px(26); line-height:px(24);
			text-align:center; font-size:px(20); background-image:linear-gradient(top,#fafafa,#f4f4f4);}
	}
	.other-inf{
		line-height:px(42); margin:0 px(8); border-bottom:solid 1px #f1f1f1; display:flex;
		span{
			display:block; flex:1; text-align:center;
			&:first-child{text-align:left;}
			&:last-child{text-align:right;}
		}
	}
	.tip-order{
		padding:0 px(8) px(8); border-bottom:solid 1px #d1d1d1; overflow:hidden;
		p{margin:px(6) 0; padding:px(6); line-height:1.6; background:#f8f8f8;}
		.view-order{margin-top:px(14); text-align:right; a{text-decoration:underline; color:#888;}}
	}
}
.pro-detail{
	line-height:1.6; background:#fff; width:px(304); margin:px(6) auto 0; padding:0 px(8); border-top:solid 1px #d1d1d1; overflow:hidden;
	h3{line-height:px(35); font-size:px(14);}
	.web-btn{margin:px(10) 0 px(18);}
}

// 订单查询
.input-form{
	width:px(300); margin:0 auto; padding:px(10) px(10) 0;
	li{margin-bottom:px(8);}
	input[type=text]{border:solid 1px #cbcbcb; border-radius:3px; font-size:px(14); padding:px(12) px(6); width:px(286);}
}
.result-show{
	width:px(300); margin:0 auto; padding:px(6) px(6) 0;
	p{font-size:px(16); border:solid 1px #cdcdcd; border-radius:5px; padding:px(10) px(8); line-height:1.6; background:#fff;}
	.main{
		border:solid 1px #cdcdcd; border-radius:5px; line-height:1.6; background:#fff;
		span{display:block;}
		.state{color:#ff6a07; padding-bottom:px(10); font-size:px(13); padding:px(10) px(8);}
		.detail{
			font-size:px(12); position:relative; line-height:1.6; padding:px(14) 0; border-top:solid 1px #dedfe0;
			background:#f8f9fa; padding:px(16) px(8) px(10); border-radius:0 0 5px 5px;
			em{
				font-size:px(10); position:absolute; left:px(4); top:px(-10); width:px(50); padding:px(4) px(8); text-align:center;
				line-height:1; border:solid 1px #d9d9d9; border-radius:5px; background:#fff;
			}
		}
	}
	.web-btn{
		margin-top:px(6); display:-webkit-box;
		a{
			display:block; -webkit-box-flex:1;
			&:first-child{margin-right:px(4);}
		}
	}
}

//search
.search-wrap .panel{width:px(320); margin:0 auto;}
.search-bar{
	padding:px(10); position:relative;
	input{padding:px(10) px(46) px(10) 0; border:solid 1px #d9d7d7; display:block; width:86%; text-indent:px(12);}
	button{width:px(20); height:px(20); border:0; position:absolute; right:px(26); top:px(22);
		background:url("../img/ico_search.jpg") no-repeat 0 0; background-size:px(20);}
}
.result-list{
	li{
		font-size:px(15); border-top:#e4e4e4 1px solid;
		a{padding:px(16) px(24) px(12) px(16);color:#232323;
		 display:block; position:relative; border:solid transparent; border-width:1px 0;}
		a:hover{
			background-color:#e5e5e5; border-color:#fafafa;
			&:before{content:""; display:block; height:px(44); width:3px; background-color:#e04931; position:absolute; left:0; top:0;}
		}
		span{display:inline-block; width:px(15); height:px(15); line-height:px(15); margin-right:px(10);
			background-color:#737373; border-radius:2px; color:#fff; font-size:px(12); text-align:center;}
		.num-top{background-color:#e05b3f;font-size:px(12);}
		a:after{content:""; display:block; position:absolute; border-radius:1px; border:solid #9f9f9f; border-width:2px 2px 0 0;
			width:px(6); height:px(6); right:px(20); top:px(18); transform:rotate(45deg);}
	}
}

//detail page
.bb-detail{
	.xiajia{background:url("../img/ico_xiajia.png") no-repeat center; background-size:px(142) px(112); position:absolute; width:100%; top:px(46); bottom:px(54);}
	.scroll-pic{padding:px(10) px(10) 0 px(10); width:px(300); height:px(304); margin:0 auto; position:relative;}
	.pic-box{
		overflow:hidden; height:px(304); border:solid #e4e7e8; border-width:1px 1px 0 1px;
		li{float:left; width:px(300); text-align:center; img{width:100%;}}
	}
	.pic-box li a{display:block;overflow:hidden;width:px(300);}
	.pic-box li img{height:px(304); width:auto; max-width:px(300);}
	.tools-bar{
		 margin:0 auto; width:px(302);
		p{padding:px(20) 0 px(4) 0; font-size:px(20); color:#171818; position:relative;}
		span{line-height:2.2rem;}
		.check{
			width:px(188); height:px(44); color:#fff; font-size:px(18); line-height:px(44); background:#ef8f3a;
			display:inline-block; text-align:center; position:absolute; right:0px; box-shadow:0 2px 1px #aeb1b2;
		}
		.favor{
			position:relative;
			a{
				position:absolute; float:right; right:px(6); top:-18.6rem;
				display:block; background:url("../img/star.png"); width:px(30); height:px(32); background-size:100% 100%;
			}
			.selected{background:url("../img/star_hover.png");background-size:100% 100%;}
		}
	}
	.handle{
		position:absolute; width:100%; z-index:1000; text-align:center; left:0;
		span{display:inline-block; width:px(12); height:px(3); background:#cdd0d0; margin:0 px(3);}
		.on{background:#e05a3f;}
	}
	.dianpu{
		width:px(286); padding:0 px(8); margin:px(10) auto 0; border:solid 1px #e4e7e8; background-color:#fff;
		h2{padding:px(6) 0;border-bottom:1px solid #eaebeb; font-size:px(12); color:#cacaca; }
		h3{font-size:px(14);}
		a{
			display:-webkit-box; padding:px(6) 0 px(8);
			&:after{content:""; display:block; width:px(8); height:px(8); border:solid #797979; border-width:2px 2px 0 0; margin:px(20) px(20) 0 0; transform:rotate(45deg);}
		}
		.pic{width:px(58); height:px(58); margin-right:px(8); overflow:hidden; background:#eee; img{width:100%; height:100%;}}
		.desc{
			-webkit-box-flex:1;height:px(58);margin-left:px(15);
			p.level{
				margin-top: 1rem; img{height:px(16); width:auto;}
			}
		}
		.desc .ico{margin-top:px(22);}
	}
	.pic_detail{
		margin:px(20) auto px(54); width:px(304); max-width:px(304); overflow:hidden;
		li{text-align:center;}
		img{max-width:px(304); width:100%;}
	}
	.ft{background-color:#fff; position:fixed; bottom:0; left:0; width:100%; height:px(56); overflow:hidden; margin:0; padding-bottom:0;}
	.ft a{display:block; width:px(250); line-height:px(44); text-align:center; margin:px(6) auto 0;
			font-size:px(17); color:#fff; background-image:linear-gradient(top, #ee8f36, #ec8b36); border-radius:5px;}
}

// 功能失效
.invalid-page{
	text-align:center; line-height:1.6;
	img{display:block; width:px(84); height:auto; margin:px(102) auto px(12);}
	p{font-size:px(14); color:#ced0d2;}
	.web-btn{
		margin-top:px(8);
		a{width:px(160);}
	}
}

// 帮助
.help-warp .info{
	margin:px(18) px(16); font-size:px(12);
	p{margin-bottom:px(10);}
	.intro{font-size:px(18);}
	.desc{line-height:px(20);}
	.special{font-size:px(16);}
	ul{
		padding-left:px(18);
		li{line-height:px(20); display:list-item; list-style-type:decimal;}
	}
	.version{margin:px(80) auto px(36); text-align:center;}
	.com{text-align:center; margin-bottom:px(4);}
	.copyright{text-align: center;}
	.contact{
		margin:auto px(54) px(24);
		.btn{
			display:block; text-align:center; font-size:px(18); width:px(180); height:px(40); line-height:px(40);
			border:solid 1px #b1b1b1; border-radius:px(10); background-image:linear-gradient(top, #fff, #eaeaea);
			&:active{background-image:linear-gradient(top,#eaeaea,#fff);}
		}
	}
}
.help-warp {
	&.custom {
		.ac {
			padding: px(3) px(3);
		}
		.info {
			margin: px(3) px(2);
		}
	}
}

// 退/换货
.select-order{
	width:px(308); margin:0 auto; padding:0 px(6);
	&.J_proDetail{margin-top:px(6);}
	.t{margin:px(10) 0 px(8); font-size:px(14); color:#787878;}
	li{margin-bottom:px(12); border-radius:5px; border:solid 1px #d9d9d9; color:#969696; box-shadow:0px 1px 3px #d9d9d9;}
	time{display:block; line-height:px(20); font-size:px(10); text-indent:px(6); border-radius:5px 5px 0 0; background:#fafafa;}
	.item-pictext{border-radius:0 0 5px 5px; padding-bottom:5px;}
	.options input{margin-top:px(40);}
}
.service-list{
	width:px(308); margin:0 auto; padding:px(6) px(6) 0;
	li{
		margin-bottom:px(6);
		&:nth-of-type(1) a{background-image:url("../img/amigo_ico_serv1.png");}
		&:nth-of-type(2) a{background-image:url("../img/amigo_ico_serv2.png");}
		&:nth-of-type(3) a{background-image:url("../img/amigo_ico_serv3.png");}
		a{
			display:block; padding:px(24) 0 px(24) px(66); font-size:px(14); position:relative; background:#fff no-repeat px(6) center;
			background-size:px(46) px(46); border-radius:5px; border:solid 1px #d9d9d9; color:#2d2d2d;
			&:after{content:""; display:block; position:absolute; top:45%; right:px(18);
				width:px(8); height:px(8); border:solid #858585; border-width:2px 2px 0 0; transform:rotate(45deg);}
		}
	}
}
.edit-order{
	width:px(308); margin:0 auto; padding:px(10) px(6) 0; color:#929292; font-size:px(14);
	li{
		margin-bottom:px(16);
		em{display:block; margin-bottom:px(6);} label, select{color:#222; font-size:px(13);}
		label{display:inline-block; vertical-align:middle;}
		.select{
			display:block; position:relative;
			&:after{content:""; display:block; position:absolute; right:px(32); top:px(14);
				width:px(10); height:px(10); border:solid #8b8b8b; border-width:0 2px 2px 0; transform:rotate(45deg);}
		}
		select{
			-webkit-appearance:none; border:solid 1px #c9c9c9; padding:px(10) 0; background:#fff; width:95%; text-indent:px(8); border-radius:3px;
			&:last-child{display:none;}
		}
		textarea{border:solid 1px #989898; border-radius:3px; padding:px(8); line-height:1.5; width:90%; min-height:px(70); font-size:px(13);}
		&:last-child{text-align:right;}
		a{color:#009cec; text-decoration:underline;}
	}
}
.submit-success{
	width:px(308); margin:0 auto; padding:px(18) px(6) 0; color:#707070;
	h3{font-size:px(14); padding:0 px(14); margin-bottom:px(6);}
	p{font-size:px(10); line-height:1.6; padding:0 px(14); margin-bottom:px(10);}
}
.rule-text{
	width:px(308); margin:0 auto; padding:px(8) px(6) 0; color:#707070;
	.title{font-size:px(13); border:solid 1px #cecece; background:#ededed; color:#292929; border-radius:5px;
		line-height:px(28); text-align:center; margin-bottom:px(8);}
	h3{font-size:px(12); margin-bottom:px(8);}
	p{font-size:px(10); line-height:1.6; padding:0 px(14); margin-bottom:px(14);}
}

.no-order{margin-top:px(30); font-size:px(15); text-align:center;}

// 翻翻页面
.ff-banner{
	width:px(320); margin:0 auto;
	.pic{padding:px(8) px(8) 0;}
	.desc{padding:px(8) px(8) 0; line-height:1.5; text-indent:px(24); color:#515151; font-size:px(14);}
}
.ff-three{
	width:px(304); margin:px(8) auto 0; padding:0 px(8);
	.pic{
		dl{display:-webkit-box;}
		dt{
			width:px(180); overflow:hidden;
			a{display:block; margin-right:px(6);}
		}
		dd{
			-webkit-box-flex:1;
			a{display:block;}
			a:first-child{margin-bottom:px(6);}
		}
	}
	.desc{padding:px(8) px(8) 0; line-height:1.5; text-indent:px(24); color:#515151; font-size:px(14);}
}
.ff-list, .ff-foot {
