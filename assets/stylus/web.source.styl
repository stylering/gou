}

// 值得买
.zdm-banner{
	width:px(320); 
	margin:0 auto;
	.pic, a {
		display: block;
		width: 100%;
		img {
			min-width: 100%;
			height: auto;
		}
	}
	.desc{
		padding:px(8) px(8) 0; 
		line-height:1.5; 
		text-indent:px(24); 
		color:#515151; 
		font-size:px(14);
	}
}
// 值得买
.zdm-list {
	width:px(310); margin:px(6) auto 0; padding: 0 px(4) 0 px(6);
	ul{overflow:hidden;}
	li{
		float:left; 
		margin-bottom:px(4);
		background: #FFF;
		&:nth-child(2n+1){
			margin-right:px(4);
		}
		a {
			display: block;
			width: px(153);
			height: px(215);
			background: #FFF;
			.pic {
				position: relative;
				text-align: center;
				height: px(153);
				overflow: hidden;
				span {
					display: table-cell;
					width: px(153);
					height: px(153);
					vertical-align: middle;
					img {
						max-width: 100%;
						width: auto;
						max-height: 100%;
						height: auto;
					}
				}
			}
			.desc {
				padding: px(6);
				.price {
					line-height: 1.2;
					em {
						color: #e2330e;
						margin-right: px(6);
					}
					u {
						font-size: px(9);
						text-decoration: line-through;
						color: #888;
					}
				}
				.discount {
					display: inline-block;
					padding: 0 5px;
					line-height: 1.5;
					text-align: center;
					font-size: px(9);
					color: #FFF;
					background: #f33442;
				}
				h3 {
					font-size: px(12);
					line-height: 1.5
