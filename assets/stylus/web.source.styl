// 分享页面
.ff-share{
	&.fixed {width: 100%; position: absolute; bottom:0; right: 0; left:0; }
	border-top:solid 1px #dadada; background:#fff; margin-top:px(10);
	.inner{
		&.custom {
			padding-bottom: px(68);
		}
		width:px(320); 
		margin:0 auto; 
		padding-bottom: px(18);
		header{
			position: relative;
			padding-left: px(12);
			height: px(48);
			line-height: px(48);
