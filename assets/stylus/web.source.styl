		float:left; width:px(150); margin-bottom:px(4);
		&:nth-child(2n+1){margin-right:px(4);}
		img{height:auto !important;}
	}
}
.ff-foot {
	.web-btn{margin-top:px(6); a{line-height:px(36); font-size:px(14); border:none;}}
	.more-links{
		margin-top:px(15);
		h3{font-size:px(12); color:#919191;}
		div{
			margin-top:px(14);
			margin-bottom:px(14);
			a{
				display:inline-block; width:px(90); line-height:px(26); text-align:center; border-radius:2px;
				color:#fff; margin-right:px(11);/*14*/
				&:nth-child(1){background:#dbd0b2;}
				&:nth-child(2){background:#bed8a9;}
